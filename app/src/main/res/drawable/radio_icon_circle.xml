<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/radio_icon_circle_background">
        <shape android:shape="oval">
            <size
                android:width="48dp"
                android:height="48dp"/>
            <solid
                android:color="#33FFFFFF"/>
            <padding
                android:top="3dp"
                android:right="3dp"
                android:bottom="3dp"
                android:left="3dp"/>
        </shape>
    </item>
    <item
        android:id="@+id/radio_icon_circle_selection_circle">
        <shape android:shape="oval">
            <stroke
                android:width="3dp"
                android:color="#FFFFFF"/>
        </shape>
    </item>
</layer-list>